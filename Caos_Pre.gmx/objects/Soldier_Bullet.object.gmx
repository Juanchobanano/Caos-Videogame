            <string>/// Attack the players.
if (y &lt; 0) instance_destroy();
if(!set_target){
    switch (estado){
        case "top":
            target_x = x + xscale * (lengthdir_x(room_width*2, 30));
            target_y = y - lengthdir_y(-room_height*2, 30);
            if (xscale &gt; 0){
                image_index = 1;
                image_yscale = -.8;
            }else{
                image_index = 0;
                image_yscale = -.8;
            }
            set_target = true;
        break;
        case "bot":
            target_x = x + xscale * (lengthdir_x(room_width, 30));
            target_y = y + lengthdir_y(-room_height, 30);
            if (xscale &gt; 0){
                image_index = 1;
            }else{
                image_index = 0;
            }
            set_target = true;
        break;
        case "mid":
            target_x = x + xscale * (lengthdir_x(room_width+200, 30));
            target_y = y
            image_index = 2;
            image_xscale = .8*xscale;
            set_target = true;
        break;
    }
    //set_target = true;
}else{
    move_towards_point(target_x, target_y, spd);
    if(collision_point(x, y, Player, true, true)){
        Hurt_Player("Soldier_Bullet", x, y);
        Play_Track("hys_bullet");
        instance_create(x, y, Explosion);
        instance_destroy();
    }
}
if(bullet_life &gt; 0){
    bullet_life--;
}else if(bullet_life &gt; 0 or speed == 0) {
    instance_destroy();
    }
   // show_debug_message(bullet_life);
</string>
