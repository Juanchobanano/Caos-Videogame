            <kind>1</kind>
            <string>/// Process input.
 dir = point_direction(x, y, mouse_x, mouse_y);
image_index = 1;
image_angle = dir;

// Process inputs.
    
    // Arriba.
    if(!Player.climb){
    
        if(Player.grounded){
            if(dir &gt;=5 and dir &lt;= 175){ // 10, 170
                with Player key_up = 1;
               // show_debug_message('up truer');;
            }else{
                with Player key_up = 0;
            }
        }else{
            if(dir &gt;=10 and dir &lt;= 170){ // 10, 170
                with Player key_up = 1;
               // show_debug_message('up truer');;
            }else{
                with Player key_up = 0;
            }
        }
            
        if(dir &gt; 180+10 and dir &lt;= 360-10){
            with Player key_down = 1;
        }else{
            with Player key_down = 0;
        }
    }else{
        if(dir &gt;= 30 and dir &lt;= 180-30){
            with Player key_up = 1;
            Check_Tutorial("GO UP!");
            
        }else{
            with Player key_up = 0;
        }
            
        if(dir &gt; 180+30 and dir &lt; 360-30){
            with Player key_down = 1;
            Check_Tutorial("GO DOWN!");
            
        }else{
            with Player key_down = 0;
        } 
    }
    
    // Derecha.
    /*}else if((dir &gt; 315 and dir &lt;= 360) or (dir &gt;= 0 and direction &lt; 45)){
        Active_Key("right");
    }*/
    
    if(dir &gt;= 270 and dir &lt; 360 or dir &gt;= 0 and dir &lt; 45){
         Active_Key("right");
         Check_Tutorial("MOVE RIGHT");
 
        // with Player key_right = 1;
    }else if(dir &gt; 135 and dir &lt; 270){
         Active_Key("left");
         Check_Tutorial("MOVE LEFT");
    }
</string>
