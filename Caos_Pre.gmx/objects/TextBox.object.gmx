    str += vel; // Speed of text.
    
}

if(str &gt;= string_length(ds_list_find_value(texto, t))){
    draw_skip = true;   
    
    if(room != game_intro and room != game10) audio_stop_sound(sn_dialogo);
    //audio_play_sound(sn_puerta, 1, 0);
    Dialogs_Final(room, t);
    if(sonidop){ audio_play_sound(sn_puerta, 1, false); sonidop = false; }
    
    //audio_play_sound(sn_puerta, 1, 0);
    
    if((keyboard_check_pressed(ord('X')) or mouse_check_button_pressed(mb_left)) and can_continue){
      
